.widget-container.background(ng-controller='CompatResultCtrl')
    button.back-to-result(ng-click='backToResult()' ng-show='!show_list') 返回结果列表
    .task-list(ng-show='show_list')
        table.taskv.compat-result
            thead.compat-result
                th.compat-result 任务状态
                th.compat-result 包名
                th.compat-result 测试台数
                th.compat-result 提交时间
                th.compat-result 更新时间
                th.compat-result 通过率
                th.compat-result 操作
                th.compat-result 提交人
            tbody.compat-result#result-body
    .report-detail(ng-show='!show_list')
        div(ng-include='"compat/report/compat-report.pug"')